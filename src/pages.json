{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue"
    }
  },
  "pages": [
    //pages 数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    { "path": "pages/index/index", "style": { "navigationBarTitleText": "首页", "navigationStyle": "custom" } },
    { "path": "pages/my/index", "style": { "navigationStyle": "custom" } }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "custom": true,
    "color": "#303133",
    "selectedColor": "#006fff",
    "backgroundColor": "#f5f5f5",
    "list": [
      { "text": "首页", "pagePath": "pages/index/index" },
      { "text": "我的", "pagePath": "pages/my/index" }
    ]
  }
}
